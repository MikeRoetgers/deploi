{
  "listen": ":8000",
  "listenSecurely": true,
  "TLS": {
    "certFile": "/etc/deploid/server.crt",
    "keyFile": "/etc/deploid/server.key"
  },
  "database": {
    "path": "/var/lib/deploid/deploid.db"
  },
  "retention": {
    "jobs": 200,
    "builds": 50,
    "buildsPerProject": {
      "exampleProject": 100
    }
  }
}
